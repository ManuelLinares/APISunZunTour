{
  "version": 2,
  "name": "sun-zun-tour",
  "alias": "apisunzuntour.now.sh",
  "public": true,
  "builds": [
    {
      "src": "app.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ],
  "env": {
    "PROD": "true",
    "PROD_URL": "sunzuntour.now.sh",
    "SGApiKey": "@test-sendgrid-api-key"
  }
}